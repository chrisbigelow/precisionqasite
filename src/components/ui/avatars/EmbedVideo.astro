---
const { src } = Astro.props;

interface Props {
  src: string;
}
---

<!-- https://www.tella.tv/video/clvwtl41w05oz0cl0fhnr04id/embed?b=0&title=0&a=1&loop=0&t=0&muted=0&wt=0 -->

<div style="position: relative; padding-bottom: 56.25%; height: 0; max-width: 800px; margin: auto;">
  <iframe style="position: absolute; top: 0; left: 0; width: 100%; height: 100%; border: 0;" src={src} allowfullscreen allowtransparency></iframe>
</div>